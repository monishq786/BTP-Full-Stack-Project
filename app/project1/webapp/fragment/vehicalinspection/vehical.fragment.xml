<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form">
    <f:SimpleForm id="customer1" editable="true" layout="ColumnLayout" columnsM="3" columnsL="4" columnsXL="5">
        <f:content>
            <core:Title text="" />
            <Label text="Plate No" />
            <Input value="{plateNo}" type="Text" />
            <Label text="Chassis No" />
            <Input value="{Chasis}" type="Text" />
            <Label text="Engine No" />
            <Input value="{Engineno}" type="Text" />
            <Label text="Manufacturer" />
            <Input value="{Manufacturer}" type="Text" />
            <Label text="Model" />
            <Input value="{Model}" type="Text" />
            <Label text="Plate Type" />
            <Input value="{paletype}" type="Text" />
            <Label text="Kind" />
            <Input value="{Kind}" type="Text" />
            <Label text="Type" />
            <Input value="{paletype}" type="Text" />

            <core:Title text="" />
            <Label text="Steering Side/ Weight Kind" />
            <Input value="{Street}" />
            <Input value="{HouseNumber}">
                <layoutData>
                    <l:GridData span="XL4 L4 M6 S4" />
                </layoutData>
            </Input>

            <Label text="In. It Reg Year/Reg Exp. Date" />
            <DatePicker id="DP3" displayFormat="short" change="handleChange" />
            <DatePicker id="DP4" displayFormat="short" change="handleChange"/>
            <Label text="Mfg. Year/Mileage" />
            <DatePicker id="DP5" displayFormat="short" change="handleChange"/>
            <!-- <Input value="{Street}" /> -->

            <Input value="{HouseNumber}">
                <layoutData>
                    <l:GridData span="XL4 L4 M6 S4" />
                </layoutData>
            </Input>
            <Label text="Horse Power / No. of Axles" />
            <Input value="{Street}" />
            <Input value="{HouseNumber}">
                <layoutData>
                    <l:GridData span="XL4 L4 M6 S4" />
                </layoutData>
            </Input>
            <Label text="No. of Cylinders/Wheels" />
            <Input value="{Street}" />
            <Input value="{HouseNumber}">
                <layoutData>
                    <l:GridData span="XL4 L4 M6 S4" />
                </layoutData>
            </Input>
            <Label text="No. of Doors/Passenger" />
            <Input value="{Street}" />
            <Input value="{HouseNumber}">
                <layoutData>
                    <l:GridData span="XL4 L4 M6 S4" />
                </layoutData>
            </Input>
            <Label text="Empty/Full Weight" />
            <Input value="{Street}" />
            <Input value="{HouseNumber}">
                <layoutData>
                    <l:GridData span="XL4 L4 M6 S4" />
                </layoutData>
            </Input>
            <Label text="Cubic Capacity" />
            <Input value="{Street}" />
            <Input value="{HouseNumber}">
                <layoutData>
                    <l:GridData span="XL4 L4 M6 S4" />
                </layoutData>
            </Input>
