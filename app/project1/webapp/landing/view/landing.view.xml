<mvc:View controllerName="landingcontroller.landing"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:core="sap.ui.core"
    xmlns:uxap="sap.uxap"
    xmlns:tnt="sap.tnt" height="100%">
    <tnt:ToolPage id="toolPage" sideExpanded="true">
        <tnt:header>

        </tnt:header>
        <tnt:mainContents>
            <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="DashBoard" subheader="Employee" press="goToDashBoard" url="https://www.sap.com/">
                <TileContent>
                    <ImageContent src="sap-icon://bbyd-dashboard" />
                </TileContent>
            </GenericTile>

            <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Employees" subheader="Master Data" press="goToEmployee" url="https://www.sap.com/">
                <TileContent>
                    <ImageContent src="sap-icon://employee" />
                </TileContent>
            </GenericTile>
             <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Users" subheader="Master Data" press="goToUser" url="https://www.sap.com/">
                <TileContent>
                    <ImageContent src="sap-icon://employee" />
                </TileContent>
            </GenericTile>
             <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Department" subheader="Master Data" press="goToDept" url="https://www.sap.com/">
                <TileContent>
                    <ImageContent src="sap-icon://employee" />
                </TileContent>
            </GenericTile>
             <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Configuration" subheader="Master Data" press="" url="https://www.sap.com/">
                <TileContent>
                    <ImageContent src="sap-icon://employee" />
                </TileContent>
            </GenericTile>
              <GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Customer" subheader="Master Data" press="goToUser" url="https://www.sap.com/">
                <TileContent>
                    <ImageContent src="sap-icon://employee" />
                </TileContent>
            </GenericTile>
            
        </tnt:mainContents>
    </tnt:ToolPage>

</mvc:View>
