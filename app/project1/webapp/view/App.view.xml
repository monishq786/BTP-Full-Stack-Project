<mvc:View controllerName="project1.controller.App"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:cor="sap.ui.core"
    xmlns:tnt="sap.tnt">
    <tnt:ToolPage id="appNew" class="sapUiDemoToolPage" sideExpanded="false">        <!-- ShellBar at the top -->
        <tnt:header>
            <f:ShellBar id="appShellBar" title="ADNOC" secondTitle="Welcome to Vehical Inspection" showMenuButton="true" showNotifications="true" notificationsPressed="true" visible="true" notificationsNumber="2" menuButtonPressed="onMenuButtonPress">
                <f:profile>
                    <Avatar src="sap-icon://log" press="" visible="true" />
                </f:profile>
            </f:ShellBar>
        </tnt:header>
        <tnt:sideContent>
            <tnt:SideNavigation id="sideNavigation" selectedKey="dashboard" itemSelect=".onNavItemSelect" expanded="false">
                <tnt:NavigationList>
                    <!-- <tnt:NavigationListItem text="Dash Board" icon="sap-icon://bbyd-dashboard" key="DashBoard"/> -->
                    <tnt:NavigationListItem text="Home" icon="sap-icon://home" key="Home"/>
                    <!-- <tnt:NavigationListGroup text="New"> -->
                        <tnt:NavigationListItem text="Search" icon="sap-icon://search" />
                        <tnt:NavigationListItem text="Service Tests" icon="sap-icon://inspection">
                            <tnt:NavigationListItem text="User Master" key="User"/>
                            <tnt:NavigationListItem text="Department Master" key="Department" />
                            <tnt:NavigationListItem text="Employee Master" key="Employee" />
                            <tnt:NavigationListItem text="Customer Master" key="Customer" />
                        </tnt:NavigationListItem>
                    <!-- </tnt:NavigationListGroup> -->
                    <tnt:NavigationListItem text="Open Service Request" icon="sap-icon://detail-view" />
                    <tnt:NavigationListItem text="Pending payment" icon="sap-icon://simple-payment" />
                </tnt:NavigationList>
            </tnt:SideNavigation>
        </tnt:sideContent>        <!-- Main Content -->
        <tnt:mainContents>
            <App id="app" />
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>