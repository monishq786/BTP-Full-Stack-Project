<mvc:View controllerName="project1.controller.customercreate"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:m="sap.m"
    xmlns:u="sap.ui.unified"
    xmlns:grid="sap.ui.layout.Grid"
    xmlns:layout="sap.ui.layout"
    xmlns="sap.uxap">

     <m:Page id="useref" title="Customer and Vehicle Creation" showNavButton="true" navButtonPress="backToLanding">
        <!-- <m:headerContent>
            <m:Label text="{view>/myName}" class="lblText boldTxt" />
            <m:Button icon="sap-icon://log" press=".onPressLogout" />
        </m:headerContent> -->

        <ObjectPageLayout upperCaseAnchorBar="false">
            <sections>
                <ObjectPageSection titleUppercase="false" title="Customer">
                    <ObjectPageSubSection title="Customer" titleUppercase="false">
                        <m:VBox id="customerSection" class="section">
                            <core:Fragment fragmentName="project1.fragment.vehicalinspection.customer" type="XML">
                            </core:Fragment>
                        </m:VBox>
                    </ObjectPageSubSection>
                </ObjectPageSection>

                <ObjectPageSection titleUppercase="false" title="Vehicle">
                    <ObjectPageSubSection title="Vehicle" titleUppercase="false">
                        <m:VBox id="Sectvehicalion" class="section">
                            <core:Fragment fragmentName="project1.fragment.vehicalinspection.vehical" type="XML">
                            </core:Fragment>
                        </m:VBox>
                    </ObjectPageSubSection>
                </ObjectPageSection>
                

            </sections>
        </ObjectPageLayout>
        <m:footer>
            <m:Toolbar>
                <m:ToolbarSpacer />
                <m:FlexBox justifyContent="End">
                    <m:Button type="Emphasized" id="user_btnSubmit" text="Submit" press="onSaveCustomerCreate"/>
                </m:FlexBox>
            </m:Toolbar>
        </m:footer>
    </m:Page>

</mvc:View>
