		</HBox>
		<footer>
			<Toolbar>
				<ToolbarSpacer />
				<FlexBox justifyContent="End">
					<Button type="Default" id="CustomerCreate" text="Create Vehicle" press="goToCreateVeh" icon="sap-icon://person-placeholder"/>
					<Button type="Emphasized" id="Retest" text="Reset" press="onSave" class="sapUiSmallMarginBegin"/>
