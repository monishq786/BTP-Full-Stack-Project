<mvc:View controllerName="project1.controller.admobility"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:card="sap.f.cards"
	xmlns:f1="sap.f" displayBlock="true"
	xmlns:w="sap.ui.integration.widgets"
	xmlns:cssgrid="sap.ui.layout.cssgrid"
	xmlns:form="sap.ui.layout.form"
	xmlns:u="sap.ui.unified"
	xmlns:commons="sap.suite.ui.commons" class="d-flex justify-content-center" height="94%">
	<Page id="enableadisableef" title="Customer Search" showNavButton="true" navButtonPress="backToLanding">
		<VBox class="sapUiSmallMargin">
			<f:SimpleForm id="SimpleFormChangeColumn_threeGroups234" editable="true" layout="ColumnLayout" title="Customer Search" columnsM="2" columnsL="3" columnsXL="4">
				<Toolbar>

					<Title text="Vehicle and Customer Details" />
					<ToolbarSpacer />
					<Button icon="sap-icon://camera" />
					<ObjectStatus text="5 Sec" state="Information" />
					<Button type="Emphasized" text="Add Mobility" press="onPress" />
					<Button type="Reject" text="Reset" press="onPress" />

				</Toolbar>
				<f:content>

					<Label text="Plate Number" />
					<Input id="name1" />

					<Label text="Plate Color/Code" />
					<Input value="{ZIPCode}">
						<layoutData>
							<f:ColumnElementData cellsSmall="3" cellsLarge="2" />
						</layoutData>
					</Input>
					<Input value="{City}" />




					<Label text="Purpose" />
					<Select id="Purpose" selectedKey="{Country}">
						<items>
							<core:Item text="" key=""/>
							<core:Item text="Re_New" key="RENEW"/>
							<core:Item text="Fresh Test" key="FRESHTEST"/>
                            <core:Item text="ESMA Test" key="FRESHTEST"/>
							<core:Item text="Traffic Test" key="FRESHTEST"/>
							<core:Item text="Comprehensive Test" key="FRESHTEST"/>
							<core:Item text="Re-Test" key="FRESHTEST"/>
							
						</items>
					</Select>
					<Label text="Main Purpose" />
                    <Input />
					<!-- <Select id="MainPurpose" selectedKey="{Country}">
						<items>
							<core:Item text="" key=""/>
							<core:Item text="Re_New" key="RENEW"/>
							<core:Item text="Fresh Test" key="FRESHTEST"/>
							
						</items>
					</Select> -->

					<Label text="" />
					<CheckBox text="Un-Registered" />

					<RadioButton text="Indivisual" selected="true" />
					<RadioButton text="Corporate" />
				</f:content>
			</f:SimpleForm>
		</VBox>
		<HBox class="sapUiMediumMargin">
			<FlexBox>
