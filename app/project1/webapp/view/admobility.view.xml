<mvc:View xmlns:tnt="sap.tnt" controllerName="project1.controller.admobility"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:card="sap.f.cards"
	xmlns:f1="sap.f" displayBlock="true"
	xmlns:w="sap.ui.integration.widgets"
	xmlns:cssgrid="sap.ui.layout.cssgrid"
	xmlns:form="sap.ui.layout.form"
	xmlns:u="sap.ui.unified"
	xmlns:commons="sap.suite.ui.commons" class="d-flex justify-content-center" height="94%">
	<Page id="enableadisableef" title="Vehicle Search" showNavButton="true" navButtonPress="backToLanding">
		<VBox class="sapUiSmallMargin">
			<f:SimpleForm id="SimpleFormChangeColumn_threeGroups234" editable="true" layout="ColumnLayout"  columnsM="3" columnsL="3" columnsXL="4">
				<Toolbar>

					<Title text="Vehicle and Customer Details" />
					<ToolbarSpacer />
					<!-- <Button icon="sap-icon://camera" />
					<ObjectStatus text="5 Sec" state="Information" /> -->
					<Button type="Emphasized" text="Add Mobility" press="onPressADMobility" />
					<Button type="Reject" text="Reset" press="onPress" />

				</Toolbar>
				<f:content>

					<Label text="Plate Number" width="10rem" />
					<Input id="name1" width="12rem"/>
					<!-- <Button icon="sap-icon://camera" width="5rem"/> -->
					<ObjectStatus text="5 Sec" state="Information" class="sapUiSmallMarginBegin" />
					<Label text="Plate Color/Code" />
					<Input value="{ZIPCode}">
						<layoutData>
							<f:ColumnElementData cellsSmall="3"  />
						</layoutData>
					</Input>
					<Input value="{City}" />




					<Label text="Purpose" />
					<Select id="Purpose" selectedKey="{Country}">
						<items>
							<core:Item text="" key=""/>
							<core:Item text="Re_New" key="RENEW"/>
							<core:Item text="Fresh Test" key="FRESHTEST"/>
							<core:Item text="ESMA Test" key="FRESHTEST"/>
							<core:Item text="Traffic Test" key="FRESHTEST"/>
							<core:Item text="Comprehensive Test" key="FRESHTEST"/>
							<core:Item text="Re-Test" key="FRESHTEST"/>

						</items>
					</Select>
					<Label text="Main Purpose" />
					<Select id="MainPurpose" selectedKey="{Country}">
						<items>
							<core:Item text="" key=""/>
						</items>
					</Select>
					<!-- <Select id="MainPurpose" selectedKey="{Country}">
						<items>
							<core:Item text="" key=""/>
							<core:Item text="Re_New" key="RENEW"/>
							<core:Item text="Fresh Test" key="FRESHTEST"/>
							
						</items>
					</Select> -->
					<Label text="" />
					<CheckBox text="Un-Registered" />

					<RadioButton text="Individual" selected="true" />
					<RadioButton text="Corporate" />
				</f:content>
			</f:SimpleForm>
		</VBox>

		<HBox class="sapUiMediumMargin">
			<FlexBox>
				<f1:Card class="sapUiMediumMargin" width="300px" height="auto">
					<!-- <f1:header>
						<card:Header title="Vehicle Details" iconSrc="sap-icon://car-rental" />

					</f1:header> -->
					<f1:content>
						<FlexBox justifyContent="SpaceAround" alignItems="Center" class="sapUiSmallMargin">
							<VBox class="sapUiSmallMarginBottom" justifyContent="SpaceAround">

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">
									<Text text="Chassis No:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/ChasisNo}" state="Information" />
								</HBox>


								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Engine No: " />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/EngineNo}" state="Information" />

								</HBox>


								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Country:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/Country}" state="Information" />

								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Manufacturer:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/Manufacturer}" state="Information" />


								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Model:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/Model}" state="Information" />

								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Kind:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/Kind}" state="Information" />

								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Type:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/Type}" state="Information" />

								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Gear Type:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/GearType}" state="Information" />


								</HBox>


								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Fuel Type:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/FuelType}" state="Information" />


								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Steering Side:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/SteeringSide}" state="Information" />


								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Weight Kind:" />
									<!-- <Text text="AUTOMATIC" /> -->
									<ObjectStatus text="{vehicleModel>/vehicleDetails/WeightKind}" state="Information" />


								</HBox>
								<HBox alignItems="Center" class="sapUiSmallMarginBottom">
									<Text text="Init Reg Year:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/InitRegYear}" state="Information" />
								</HBox>

							</VBox>

						</FlexBox>
					</f1:content>
				</f1:Card>
				<f1:Card class="sapUiMediumMargin" width="300px" height="auto">

					<f1:content>
						<FlexBox justifyContent="SpaceAround" alignItems="Center" class="sapUiSmallMargin">
							<VBox class="sapUiSmallMarginBottom" justifyContent="SpaceAround">

								


								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Reg Exp. Date:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/ChasisNo}" state="Information" />


								</HBox>


								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Plate Type: " />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/PlateType}" state="Information" />

								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="MFG Year:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/MfgYear}" state="Information" />


								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Horse Power:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/HorsePower}" state="Information" />


								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="No. of Axies:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/NoOfAxies}" state="Information" />

								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="No. of Cylinders:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/NoOfCylinders}" state="Information" />

								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="No. of Wheels:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/NoOfWheels}" state="Information" />

								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="No. of Passengers:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/NoOfPassengers}" state="Information" />


								</HBox>
								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Empty Weight:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/EmptyWeight}" state="Information" />


								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Full Weight:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/FullWeight}" state="Information" />


								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Mileage:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/Mileage}" state="Information" />


								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Cubic Capacity:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/CubicCapacity}" state="Information" />


								</HBox>

							</VBox>
							<HBox>

							</HBox>
						</FlexBox>
					</f1:content>
				</f1:Card>



				<f1:Card class="sapUiMediumMargin" width="300px" height="auto">
					<f1:header>

						<card:Header title="Customer Details" iconSrc="https://ui5.sap.com/test-resources/sap/ui/integration/demokit/sample/CardsLayout/images/Avatar_1.png" />

					</f1:header>

					<f1:content>
						<FlexBox justifyContent="SpaceAround" alignItems="Center" class="sapUiSmallMargin">
							<VBox class="sapUiSmallMarginBottom" justifyContent="SpaceAround">

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">
									<Text text="Name:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/Name}" state="Information" />
								</HBox>


								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Email:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/Email}" state="Information" />

								</HBox>


								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Mobile:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/Mobile}" state="Information" />

								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="City:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/City}" state="Information" />


								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Postal Code:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/Postal}" state="Information" />

								</HBox>

								<HBox alignItems="Center" class="sapUiSmallMarginBottom">

									<Text text="Address:" />
									<ObjectStatus text="{vehicleModel>/vehicleDetails/Address}" state="Information" />

								</HBox>


							</VBox>
						</FlexBox>
					</f1:content>
				</f1:Card>

			</FlexBox>
		</HBox>
		<footer>
			<Toolbar>
				<ToolbarSpacer />
				<FlexBox justifyContent="End">
					<Button type="Default" id="CustomerCreate" text="Create Vehicle" press="goToCreateVeh" icon="sap-icon://add-employee"/>
					<Button type="Emphasized" id="Retest" text="Retest" press="onSave" class="sapUiSmallMarginBegin"/>
					<Button type="Emphasized" id="Proceed" text="Proceed" press="onSaveSO" class="sapUiSmallMarginBegin"/>
				</FlexBox>
			</Toolbar>
		</footer>

	</Page>
</mvc:View>